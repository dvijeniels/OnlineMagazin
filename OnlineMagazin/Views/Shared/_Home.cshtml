                                    <form action="#">
                                        <input type="text" placeholder="Поиск">
                                        <button type="submit"><i class="fa fa-search"></i></button>
                                    </form>
                                </div>
                            </div>
                            <div style="display: inline-block;" id="followersrefresh">
                                @await Component.InvokeAsync("CartView")
